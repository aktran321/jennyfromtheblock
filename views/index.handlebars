<div class="container">
  <div class="jumbotron jumbo-logo">
    <div class="container">
     <!-- <img src="/assets/img/chzbgr-sequel-logo.svg" class="chzbgr-logo"> -->
    </div>
  </div>
</div>

<div class="container">
  <div class="row">
    {{!-- SUBJECT OPTIONS --}}
    <div class="col-6">
      <div class="jumbotron classes-to-take">
        <h4>Classes</h4>
        <ul class="list-group list-group-flush">
          {{#each subjects}} {{#unless inSchedule}}
          <li class="list-group-item">
            {{subject_name}}
            <button class="btn btn-secondary not-in-schedule" data-id="{{id}}">ADD</button>
          </li>
          {{/unless}}{{/each}}
          <hr>
        </ul>
      </div>
    </div>

    {{!-- SUBJECTS ADDED --}}
    <div class="col-6">
      <div class="jumbotron classes-added">
        <h4>Burgers Eaten</h4>
        <ul class="list-group list-group-flush">
          {{#each subjects}} {{#if inSchedule}}
          <li class="list-group-item">
            {{subject_name}}
            <button class="btn btn-secondary in-schedule" data-id="{{id}}">DELETE</button>
          </li>
          {{/if}}{{/each}}
          <hr>
        </ul>
      </div>
    </div>
  </div>
</div>

{{!-- ADD A NEW Subject --}}
<div class="container">
  <div class="row">
    <div class="col-12">
      <div class="jumbotron class-add">
        <h4>Pick your subjects</h4>
        <form>
          <div class="form-group">
            <label for="add-subject"></label>
            <input type="text" class="form-control" id="add-subject" placeholder="">
            <button type="submit" class="add-class btn btn-secondary">Add subject</button>
          </div>
        </form>
      </div>
    </div>
  </div>
</div>

<div class="container-fluid footer">
  <div class="row">
    <div class="col-12">
      <hr>
      Created by Alex Simuro, Andy Tran, Emma Stotz, Juan Munoz, and Sandy Enow
    </div>
  </div>
</div>
<div class = "container">
    <div id = "timetable" class="timetable"></div>
</div>